{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/next" },
  "defaultProject": "chat",
  "generators": {
    "@nrwl/react": { "application": { "babel": true } },
    "@nrwl/next": {
      "application": { "style": "@emotion/styled", "linter": "eslint" }
    }
  },
  "projects": {
    "chat": {
      "root": "apps/chat",
      "sourceRoot": "apps/chat",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": { "root": "apps/chat", "outputPath": "dist/apps/chat" },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "chat:build", "dev": true },
          "configurations": {
            "production": {
              "buildTarget": "chat:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "chat:build:production" }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/chat"],
          "options": {
            "jestConfig": "apps/chat/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/chat/**/*.{ts,tsx,js,jsx}"] }
        }
      }
    },
    "chat-e2e": {
      "root": "apps/chat-e2e",
      "sourceRoot": "apps/chat-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/chat-e2e/cypress.json",
            "tsConfig": "apps/chat-e2e/tsconfig.e2e.json",
            "devServerTarget": "chat:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "chat:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/chat-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "components": {
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/components/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/components"],
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "hooks": {
      "root": "libs/hooks",
      "sourceRoot": "libs/hooks/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/hooks/**/*.{ts,tsx,js,jsx}"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/hooks"],
          "options": {
            "jestConfig": "libs/hooks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api": {
      "root": "libs/api",
      "sourceRoot": "libs/api/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/api/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api"],
          "options": {
            "jestConfig": "libs/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "embedded": {
      "root": "libs/embedded",
      "sourceRoot": "libs/embedded/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/embedded",
            "tsConfig": "libs/embedded/tsconfig.lib.json",
            "project": "libs/embedded/package.json",
            "entryFile": "libs/embedded/src/index.ts",
            "external": [
              "react",
              "react-dom",
              "@emotion/styled",
              "@emotion/react"
            ],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "libs/embedded/rollup.config.js",
            "assets": [{ "glob": "README.md", "input": ".", "output": "." }]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/embedded/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/embedded"],
          "options": {
            "jestConfig": "libs/embedded/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
